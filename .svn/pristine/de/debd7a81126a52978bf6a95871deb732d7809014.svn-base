                hotRank() {
                    var _this = this;
                    // 保险热销
                    var insuranceType = {
                        risktype: this.activeName,
                        title: "",
                        pageNumber: 1,
                        pageSize: 4,
                        pageTotal: 0,
                        salesArea: "",
                    };
                    var res = ajaxMethod('getHomePageRiskList', insuranceType);
                    let {
                        rows,
                        total
                    } = res;
                    _this.riskList = res.rows;

                },

                // 选择文章类型
                articleTypeClick(tab, event) {
                    let _this = this;
                    this.articleTypelabel = tab.$props.label
                    this.articleRank()
                },

                // 获取文章内容
                articleRank() {
                    var _this = this
                    // 保险热销
                    var insuranceType = {
                        type: this.articleTypeName,
                        startDate: '',
                        endDate: '',
                        pageNumber: 1,
                        pageSize: 5,
                    }
                    if (this.articleTypeName == '00') {
                        insuranceType.type = '';
                        insuranceType['istop'] = 'Y'
                    }
                    var res = ajaxMethod('getHomePageContentList', insuranceType);
                    let {
                        rows,
                        total
                    } = res;
                    _this.articleList = res.rows;
                },


                srarch() {
                    window.location.href = " https://www.un29.com/#/search?searchValue=" + this.searchValue
                },

                navigateToSomewhere(actileId) {
                    window.location.href = "https://www.un29.com/#/ArticleDetailes/" + actileId
                },
                menuClick(num) {
                    if (num == 1) {
                        window.location.href = "https://www.un29.com/#/ArticleList/" + this.articleTypeName
                    } else if (num == 2) {
                        window.location.href = "https://www.un29.com/#/aboutUs"
                    } else if (num == 3) {
                        window.location.href = "https://www.un29.com/#/message"
