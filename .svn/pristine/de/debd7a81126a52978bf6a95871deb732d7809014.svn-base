<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>首页</title>
    <link rel="stylesheet" href="./css/common.css" />
    <link rel="stylesheet" href="./css/elementUI.css" />

    <script>
        !(function () {
            function a() {
                var html = document.documentElement;
                var w = html.getBoundingClientRect().width;
                if (w > 750) {
                    w = 750;
                }
                var f = w / 7.5;
                html.style.fontSize = f + "px";
            }
            a();
            var timer;
            window.onresize = function () {
                clearTimeout(timer);
                timer = setTimeout(a, 300);
            };
        })();
    </script>
    <style>
        .template {
            width: 7.5rem;
            background-color: #f5f7ff;
            font-size: 0.32rem;
            margin: 0 auto;
        }

        .home {
            width: 7.5rem;
        }

        .head {
            width: 100%;
            height: 5.6rem;
            background: url(./images/bg.png) no-repeat center center;
            background-size: 100% 100%;
            padding: 0 0.35rem;
            box-sizing: border-box;
        }

        .navLogo {
            height: 1.3rem;
            display: flex;
            justify-content: center;
            align-items: center;
        }

        .navLogo img {
            width: 1.92rem;
            height: 0.53rem;
            margin-left: 30%;
        }

        .navLogo .el-icon-s-operation {
            font-size: 0.36rem;
            margin-left: 30%;
            float: right;
        }



        .icon-menu {
            width: 0.4rem;
            height: 0.06rem;
            border-top: 0.18rem double #0C0C0E;
            border-bottom: 0.06rem solid #0C0C0E;
            margin-left: 30%;
        }

        .searchBox {
            width: 100%;
            height: 0.6rem;
            line-height: 0.6rem;
            margin: 0 auto;
        }

        .searchInput {
            width: 100%;
            height: 0.6rem;
            background: #fff;
            border-radius: 0.28rem;
            display: flex;
            justify-content: space-between;
            align-items: center;
        }

        .searchInput i {
            font-size: 0.26rem;
            color: #dcdfe6;
            margin-left: 0.25rem;
        }

        .searchInput .el-input {
            display: flex;
            width: 72%;
            outline: none;
        }

        .searchInput input {
            height: 0.5rem;
            line-height: 0.5rem;
            border: 0;
            outline: none;
            font-size: 0.26rem;
        }

        .searchInput .srarchBtn {
            width: 1.3rem;
            height: 0.48rem;
            line-height: 0.48rem;
            background: #3244a8;
            border-radius: 0.24rem;
            color: #fff;
            font-size: 0.26rem;
            text-align: center;
            margin-right: 0.05rem;
        }

        input::-webkit-input-placeholder {
            line-height: 0.5rem;
            color: #959595;
            font-size: 0.26rem;
        }

        input::-moz-placeholder {
            line-height: 0.5rem;
            color: #959595;
            font-size: 0.26rem;
        }

        input::moz-placeholder {
            line-height: 0.5rem;
            color: #959595;
            font-size: 0.26rem;
        }

        input::-ms-input-placeholder {
            line-height: 0.5rem;
            color: #959595;
            font-size: 0.26rem;
        }

        .info h3 {
            margin-top: 0.45rem;
            margin-bottom: 0.2rem;
        }

        .info p {
            color: #525151;
            font-size: 0.22rem;
            width: 62%;
        }

        .info .zixun {
            width: 1.9rem;
            height: 0.56rem;
            line-height: 0.56rem;
            display: flex;
            justify-content: center;
            align-items: center;
            color: #fff;
            background: #3244a8;
            font-size: 0.28rem;
            margin-top: 0.32rem;
            border-radius: 0.28rem;
        }

        .zixun p {
            color: #fff;
            text-align: center;

        }

        .main {
            width: 6.8rem;
            height: auto;
            margin: 0.2rem auto;

        }

        .menu {
            height: 2.1rem;
            background: #fff;
        }

        .menu ul {
            height: 100%;
            display: flex;
            justify-content: space-around;
            align-items: center;
        }

        .menu ul li .menuItem {
            text-align: center;
            font-size: 0.26rem;
            color: #323232;
        }

        .menu ul li .menuItem img {
            width: 0.88rem;
            height: 0.88rem;
        }

        .product {
            background: #fff;
            font-size: 0.24rem;
            margin-top: 0.2rem;
            padding: 0.3rem 0.2rem;
        }

        .proTitle {
            display: flex;
            justify-content: space-between;
            align-items: center;
            font-size: 0.36rem;
        }

        .proTitle .rankTitle {
            font-weight: bold;
            font-size: 0.36rem;
        }

        .rankTitle:before {
            content: "";
            padding-left: 0.15rem;
            border-left: 5px solid #3244a8;
            height: 0.28rem;
            margin-top: -5px;
            display: inline-block;
            vertical-align: middle;
        }

        .proTitle .rankMore {
            font-size: 0.24rem;
            color: #3244a8;
        }

        .product .el-tabs__item {
            font-size: 0.32rem;
            padding: 0 0.16rem;
        }

        .product .el-tabs__nav {
            width: 100%;
            height: 0.8rem;
            display: flex;
            justify-content: space-between;
            margin-top: 0.22rem;
        }

        .product .is-active {
            color: #3244a8;
            font-weight: bold;
        }

        .product .el-tabs__active-bar {
            background-color: #3244a8;
            height: 4px;
        }

        .bottomTabs .el-tabs__nav-wrap::after {
            position: static !important;
        }

        .bottomTabs .el-tabs__active-bar {
            /* background-color: transparent !important; */

        }


        .insuranceType {
            display: flex;
            flex-direction: column;
            justify-content: space-between;
            align-items: center;
        }

        .insuranceType li {
            width: 6.4rem;
            height: 2.4rem;
            background: #f5f7ff;
            /* box-shadow: 0px 0px 3px 0px #bbbbbb; */
            margin-bottom: 0.26rem;
            border-radius: 5px;
            cursor: pointer;
            padding: 0.2rem;
            box-sizing: border-box;
            overflow: hidden;
        }

        .insureBox {
            height: 100%;
            display: flex;
            justify-content: space-between;
        }

        .insure_leftBox {
            width: 100%;
            height: 100%;
            display: flex;
            flex-direction: column;
            justify-content: space-between;
            /* margin-bottom: 30px; */
        }

        .insure_rightBox img {
            margin-top: 20px;
        }

        .insure_leftBox h3 {
            font-weight: 700;
            color: #000000;
            font-size: 0.32rem;
            text-align: left;
            margin-bottom: 0.1rem;
            width: 100%;
            white-space: nowrap;
            /* 不换行 */
            overflow: hidden;
            /* 隐藏溢出内容 */
            text-overflow: ellipsis;
        }

        .insure_leftBox .insDec {
            width: 100%;
            overflow: hidden;
            text-overflow: ellipsis;
            display: -webkit-box;
            -webkit-line-clamp: 2;
            -webkit-box-orient: vertical;

            font-size: 0.26rem;
            color: #525151;
            text-align: left;
        }

        .comany_logo {
            display: flex;
            justify-content: space-between;
            align-items: center;
        }

        .comany_logo img {
            width: 1.68rem;
            height: 0.44rem;
            margin-left: -0.55rem;
        }

        .insMoney {
            text-align: left;
            color: #000000;
            font-size: 0.26rem;
        }

        .insMoney span:first-child {
            color: #da251e;
            font-size: 0.26rem;


        }

        .insMoney span:last-child {
            color: #da251e;
            font-size: 0.49rem;
            font-weight: bold;

        }

        .rank_row {
            margin: 0.2rem auto;
        }

        .reser img {
            width: 6.8rem;

        }


        .hotRank-top .el-tabs__active-bar {
            display: none;
        }

        .hotRank-top .el-tabs__nav-wrap::after {
            position: static !important;
        }

        .hotRank-top .el-tabs__nav {
            width: 100%;
            height: auto;
            display: flex;
            flex-wrap: wrap;
            justify-content: flex-start;
            margin-top: 0.22rem;
        }

        .hotRank-top .articleTypeList .el-tabs__item {
            width: 1.5rem;
            height: 0.48rem;
            line-height: 0.48rem;
            background: #F5F7FF;
            text-align: center;
            padding: 0;
            font-weight: bold;
            margin-right: 0.1rem;
            margin-bottom: 0.15rem;
            font-size: 0.26rem;
        }

        .hotRank-top .articleTypeList .el-tabs__item.is-active {
            width: 1.5rem;
            height: 0.48rem;
            line-height: 0.48rem;
            background: #3244a8;
            color: #fff;
            text-align: center;
            padding: 0;
            margin-right: 0.1rem;
        }

        .hotRank-top .articleTypeList .el-tabs__item:hover {
            width: 1.5rem;
            height: 0.48rem;
            line-height: 0.48rem;
            background: #3244a8;
            color: #fff;
            text-align: center;
            padding: 0;
            margin-right: 0.1rem;
        }

        .dianzan {
            display: flex;
            justify-content: center;
            align-items: center;
        }

        .zan {
            width: 0.2rem;
            height: 0.19rem;
            background: url(./images/zan_01.png) no-repeat center center;
            background-size: 100% 100%;
            margin-right: 2px;
        }

        .is-active .zan {
            width: 0.2rem;
            height: 0.19rem;
            background: url(./images/zan_02.png) no-repeat center center;
            background-size: 100% 100%;
        }


        .articleList li {
            width: 6.6rem;
            height: 2.29rem;
            background: #ffffff;
            border-radius: 5px;
            margin-bottom: 0.2rem;
            /* padding: 0.2rem; */
            box-sizing: border-box;
            display: flex;
            justify-content: space-between;
            align-items: center;
        }

        .article_left {
            width: 2.5rem;
            height: 1.9rem;
        }

        .article_left img {
            height: 100%;
            margin: 0 auto;
            max-width: 2.4rem;
            object-fit: cover;
        }

        .article_right {
            width: 4.1rem;
            display: flex;
            height: 1.85rem;
            flex-direction: column;
            justify-content: space-between;
            font-size: 0.26rem;
            margin-left: 0.25rem;
        }

        .article_right h3 {
            font-weight: 700;
            text-align: left;
            width: 100%;
            overflow: hidden;
            text-overflow: ellipsis;
            display: -webkit-box;
            -webkit-line-clamp: 2;
            -webkit-box-orient: vertical;
            margin-bottom: 6px;
            font-size: 0.32rem;
        }

        .article_right p {
            text-align: left;
            color: #525151;
        }

        .article_right .article_dec {
            width: 100%;
            white-space: nowrap;
            /* 不换行 */
            overflow: hidden;
            /* 隐藏溢出内容 */
            text-overflow: ellipsis;
            /* 使用省略号 */
        }

        .comany p {
            color: #525151;
            font-size: 0.26rem;
            margin: 0.18rem auto;
        }

        /* 底部开始 */

        .foot {
            width: 7.5rem;
            background: #333437;
            color: #fff;
            height: 4rem;
            margin: 0 auto;
            padding: 0.5rem 0.35rem;
            box-sizing: border-box;
            font-size: 0.22rem;
        }

        .footBox {
            display: flex;
            justify-content: space-between;
            margin-bottom: 0.6rem;
        }

        .foot_left {
            display: flex;
            flex-direction: column;
            justify-content: space-around;
        }

        .foot_right img {
            width: 1.5rem;
            height: 1.5rem;
        }

        .foot_right p {
            text-align: center;
        }

        .jing img {
            width: 0.25rem;
            height: 0.25rem;
        }

        .jing .mess {
            text-align: center;
        }


        /* 弹窗 */


        .bujv {
            display: flex;
            flex-direction: column;
            align-items: center;
        }



        /* .footMain {
            width: 1200px;
            display: flex;
            justify-content: space-between;
            margin-top: 80px;
        } */

        .foot_title {
            font-size: 0.44rem;
            display: flex;
            color: #fff;
            margin-bottom: 0.3rem;
        }

        .foot_title p {
            margin-left: 1.7rem;
        }



        .foot_right p {
            /* color: #fff;
            font-size: 22px;
            margin-top: 20px; */
        }

        .footer-copyright {
            /* color: #fff;
            font-size: 20px; */
        }

        /*  */

        .popup {
            width: 100%;
            height: 2rem;
            background: #5483fd;
            opacity: 0.9;
            /* position: fixed;
             bottom: 0; */
            z-index: 1000;
        }

        .foot_popup {
            display: flex;
            justify-content: space-between;
            z-index: 1001;
        }

        .close {
            position: fixed;
            bottom: 1.6rem;
            right: 0.3rem;
            z-index: 1001;
            color: #fff;
            font-size: 0.4rem;
            cursor: pointer;
        }

        .footBujv-left {
            width: 0.35rem;
            /* border: 1px solid red; */
            display: flex;
            align-items: end;
        }

        .footBujv-right {
            /* border: 1px solid red; */
            /* width: 450px; */
            /* height: 380px; */
            background: #fff;
            border-radius: 10px;
            color: #525151;
        }



        .popup_right p:first-child {
            font-size: 24px;
            text-align: center;
            margin-bottom: 25px;
        }

        .insureName {
            display: flex;
            margin-bottom: 16px;
        }

        .insureName .text {
            width: 1.2rem;
            height: 0.6rem;
            line-height: 0.6rem;
            font-size: 0.24rem;
            text-align: justify;
            white-space: pre;
        }

        .insureName .el-input {
            width: 4.5rem;
            height: 0.6rem;
            line-height: 0.6rem;
            border-radius: 10px;
            border: 1px solid #dddddd;
        }

        .insureName .el-input__inner {
            height: 0.4rem;
            line-height: 0.4rem;
            border: 0;
            border-radius: 10px;
            font-size: 0.2rem;
        }

        .verification-code .el-input {
            width: 2.2rem;
            border: 0;
        }

        .insureName .nameValue {
            width: 4.5rem;
            height: 0.6rem;
            line-height: 0.6rem;
            border-radius: 10px;
            border: 1px solid #dddddd;
            display: flex;
            justify-content: space-between;
        }

        .send {
            color: #3244a8;
            line-height: 0.6rem;
            padding: 0 24px;
            font-size: 0.24rem;
            cursor: pointer;
        }

        .tijiao {
            border-radius: 0.28rem;
            color: #fff;
            font-size: 0.28rem;
            font-weight: bold;
            margin: 0 auto;
            margin: 0.23rem auto;
            width: 5rem;
            height: 0.7rem;
            line-height: 0.7rem;
            background: #3244a8;
            border-radius: 0.37rem;
            text-align: center;
        }

        .popup_right .dec {
            margin: 0.2rem auto 0.35rem;
            font-size: 0.26rem;
            text-align: center;
        }

        .popup_right p:last-child {
            font-size: 0.22rem;
            color: #525151;
            text-align: center;
        }

        /* 设置所有输入字段的占位文本字体大小 */
        ::v-deep input::-webkit-input-placeholder {
            font-size: 18px;
            /* Chrome, Safari, Opera */
        }

        ::v-deep input:-moz-placeholder {
            font-size: 18px;
            /* Firefox 18- */
        }

        ::v-deep input::-moz-placeholder {
            font-size: 18px;
            /* Firefox 19+ */
        }

        ::v-deep input:-ms-input-placeholder {
            font-size: 18px;
            /* Internet Explorer 10+ */
        }

        .actile p {
            line-height: 0.25rem;
            width: 100%;
            padding-bottom: 10px;
            -webkit-box-sizing: border-box;
            box-sizing: border-box;
            font-size: 16px;
            color: #666;
            letter-spacing: 0;
            text-align: justify;
            line-height: 30px;
            font-weight: 400;
        }

        .actile ul {
            margin-bottom: 10px;
        }

        .actile ul li {
            font-size: 16px;
            color: #666;
            padding: 2px 20px;
            text-indent: 1em;
        }

        .infoMengess {
            color: #3244a8;
            cursor: pointer;
        }

        .popup_right h1 {
            text-align: center;
            font-size: 0.38rem;
        }

        .el-message {
            font-size: 0.25rem;
        }

        .el-message--error .el-message__content {
            font-size: 0.2rem;
        }




        .sideNav {
            position: fixed;
            top: 50%;
            right: 0;
            z-index: 2000;
        }

        .sideNav_box1 img {
            width: 1.2rem;
            height: 1.2rem;

        }
    </style>
</head>

<body>
    <div id="app">
        <div class="template">
            <div class="home">
                <div class="head">
                    <div class="navLogo">
                        <img src="./images/logo.png" alt="" />
                        <!-- <i class="el-icon-s-operation"></i> -->

                        <div class="icon-menu"></div>
                    </div>
                    <div class="searchBox">
                        <div class="searchInput">
                            <i class="el-icon-search"></i>
                            <el-input placeholder="请输入你想搜索的关键词" v-model="searchValue">
                            </el-input>
                            <div class="srarchBtn" @click="srarch">搜索</div>
                        </div>
                    </div>
                    <div class="info">
                        <h3>第三方保险服务持牌机构</h3>
                        <p>
                            始于2008年11月，经国家金融监督管理总
                            局批准设立。总部位于上海，战略合作全
                            国多达80余家保险公司，提供近千款产品
                            甄选服务，服务用户超过3000万人次。
                        </p>

                        <div class="zixun" @click="openPopup()">
                            <p>方案咨询</p>
                            <i class="el-icon-caret-right"></i>
                        </div>
                    </div>
                </div>

                <div class="main">
                    <div class="menu">
                        <ul>
                            <li @click="menuClick(1)">
                                <div class="menuItem">
                                    <img src="./images/menu01.png" alt="" />
                                    <p>保险课堂</p>
                                </div>
                            </li>

                            <li @click="menuClick(2)">
                                <div class="menuItem">
                                    <img src="./images/menu02.png" alt="" />
                                    <p>关于我们</p>
                                </div>
                            </li>
                            <li @click="menuClick(3)">
                                <div class="menuItem">
                                    <img src="./images/menu03.png" alt="" />
                                    <p>信息披露</p>
                                </div>
                            </li>
                        </ul>
                    </div>

                    <div class="product">
                        <div class="proTitle">
                            <p class="rankTitle">保险热销</p>
                            <p class="rankMore" @click="productClick()">查看更多></p>
                        </div>

                        <div class="bottomTabs">
                            <el-tabs v-model="activeName" @tab-click="handleClick" class="tabs">
                                <el-tab-pane label="意外" name="01"></el-tab-pane>
                                <el-tab-pane label="医疗" name="02"></el-tab-pane>
                                <el-tab-pane label="重疾" name="03"></el-tab-pane>
                                <el-tab-pane label="定寿" name="04"></el-tab-pane>
                                <el-tab-pane label="理财" name="05"></el-tab-pane>
                                <el-tab-pane label="车险" name="06"></el-tab-pane>
                                <el-tab-pane label="其他" name="07"></el-tab-pane>
                            </el-tabs>
                        </div>

                        <div class="tabMain">
                            <div class="tabMain_box">
                                <ul class="insuranceType" v-if="riskList.length>0">
                                    <li v-for="item in riskList" :key="item.riskcode">
                                        <a :href="item.url" class="insureBox" target="_bank">
                                            <div class="insure_leftBox">
                                                <div>
                                                    <h3>{{ item.title }}</h3>
                                                    <p class="insDec">{{ item.description }}</p>
                                                </div>

                                                <div class="comany_logo">
                                                    <img :src="item.logo" alt="" />
                                                    <p class="insMoney">
                                                        <span>￥</span><span>{{item.prem}}</span>/首年起
                                                    </p>
                                                </div>
                                            </div>
                                        </a>
                                    </li>
                                </ul>

                                <div class="insuranceType" v-else>
                                    <div>
                                        <img src="./images/No.png" alt="" style="width: 100%;" />
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>



                    <div class="rank_row">
                        <div class="reser" @click="openPopup()">
                            <img src="./images/yuyueBanner.png" alt="">
                        </div>
                    </div>

                    <div class="product">
                        <div class="proTitle">
                            <p class="rankTitle">内容精选</p>
                            <p class="rankMore" @click="menuClick(1)">查看更多></p>
                        </div>

                        <div class="hotRank-top ">
                            <div class="top-main">
                                <el-tabs v-model="articleTypeName" @tab-click="articleTypeClick"
                                    class="articleTypeList">
                                    <el-tab-pane label="推荐" name="00">
                                        <div slot="label" class="dianzan">
                                            <p class="zan"></p>
                                            <p>推荐</p>
                                        </div>
                                    </el-tab-pane>
                                    <el-tab-pane label="产品测评" name="01"></el-tab-pane>
                                    <el-tab-pane label="保险科普" name="02"></el-tab-pane>
                                    <el-tab-pane label="挑选指南" name="03"></el-tab-pane>
                                    <el-tab-pane label="避坑专题" name="04"></el-tab-pane>
                                    <el-tab-pane label="产品榜单" name="05"></el-tab-pane>
                                    <el-tab-pane label="理财观念" name="06"></el-tab-pane>
                                    <el-tab-pane label="保障观念" name="07"></el-tab-pane>
